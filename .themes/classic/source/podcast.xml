<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" media="screen" href="/~d/styles/rss2enclosuresfull.xsl"?><?xml-stylesheet type="text/css" media="screen" href="http://feeds.feedburner.com/~d/styles/itemcontent.css"?>
<rss xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:wfw="http://wellformedweb.org/CommentAPI/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:sy="http://purl.org/rss/1.0/modules/syndication/" xmlns:slash="http://purl.org/rss/1.0/modules/slash/" xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" xmlns:rawvoice="http://www.rawvoice.com/rawvoiceRssModule/" xmlns:media="http://search.yahoo.com/mrss/" xmlns:feedburner="http://rssnamespace.org/feedburner/ext/1.0" version="2.0">

    <channel>
        <title>{{ site.title }}</title>
	
        <link>{{ site.url }}</link>
        <description></description>
        <lastBuildDate>{{Time.now.strftime("%a, %d %b %Y %H:%M:%S %z")}}</lastBuildDate>
        <language>de</language>
        <sy:updatePeriod>hourly</sy:updatePeriod>
        <sy:updateFrequency>1</sy:updateFrequency>
	
        <itunes:summary>{{ site.summary }}</itunes:summary>
        <itunes:author>{{ site.author }}</itunes:author>
        <itunes:explicit>no</itunes:explicit>
        <itunes:image href="{{site.logo}}" />
        <itunes:owner>
            <itunes:name>{{ site.author }}</itunes:name>
            <itunes:email>{{ site.email }}</itunes:email>
        </itunes:owner>
        <managingEditor>{{site.email}} {{site.author}}</managingEditor>
        <copyright>Creative Commons BY-NC-SA 3.0 DE</copyright>
        <itunes:subtitle>{{ site.subtitle }}</itunes:subtitle>
        <itunes:keywords>{{ site.keywords.to_a.join(', ') }}</itunes:keywords>
        <image>
            <title>{{ site.title }}</title>
            <url>{{ site.logo }}</url>
            <link>{{ site.url }}</link>
        </image>
        {% for category in site.categories %}
        <itunes:category text="{{ category }}">
        {% endfor %}    
        <media:copyright>Creative Commons BY-NC-SA 3.0 DE</media:copyright>
        <media:thumbnail url="{{ site.url }}" />
        <media:keywords>{{ site.keywords.to_a.join(', ') }}</media:keywords>
        <media:category scheme="http://www.itunes.com/dtds/podcast-1.0.dtd">{{site.categories.to_a.first}}</media:category>
        {% for post in site.posts limit: 20 %}
        <item>
            <title>{{ post.title }}</title>
            <link>{{ site.url }}{{ post.url }}</link>
            <comments>{{ site.url }}{{ post.url }}#comments</comments>
            <pubDate>{{post.date.strftime("%a, %d %b %Y %H:%M:%S %z")}}</pubDate>
            <dc:creator>{{post.author}}</dc:creator>
            {% for category in post.categories %}
            <category><![CDATA[{{ category }}]]></category>
            {% endfor %}
            <guid isPermaLink="false">{{ site.url }}{{ post.url }}</guid>
            <description><![CDATA[{{ post.content | expand_urls: site.url | cdata_escape }}]]></description>
            <content:encoded><![CDATA[{{ post.content | expand_urls: site.url | cdata_escape }}]]></content:encoded>
            <enclosure url="{{ post.audio }}" length="0" type="audio/mpeg" />
            <itunes:keywords>{{ post.tags }}</itunes:keywords>
            <itunes:subtitle></itunes:subtitle>
            <itunes:summary></itunes:summary>
            <itunes:author>Binaergewitter Crew</itunes:author>
            <itunes:explicit>no</itunes:explicit>
            <itunes:duration></itunes:duration>
            <media:content url="{{ post.audio }}" fileSize="0" type="audio/mpeg" />
        </item>
        {% endfor %}
        <media:credit role="author">Binaergewitter Crew</media:credit>
        <media:rating>nonadult</media:rating>
        <media:description type="plain">Something, something, something, cheese</media:description>
    </channel>
</rss>
